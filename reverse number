#reverse integer

def reverse(self, x):
        rev=0
        if (x>0):
            a=1
        else: 
            a=-1      
        num=a*x   
        while(num>0):
            rev=rev*10+num%10
            num=num/10
        rev=rev*a
            
        if (rev > 2**31-1 or rev < -2**31):
            return 0
        return rev
